                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Navigator" />
        </activity>
        <activity
            android:name=".SingleActivity"
            android:parentActivityName=".Navigator">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Navigator" />
        </activity>
        <activity
            android:name=".SignUp"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".SignIn"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".Navigator"
